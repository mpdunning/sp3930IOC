
record( stringin, "$(P):SI:IOCNAME") {
  field( PINI, "YES")
  field( VAL,  "$(IOCNAME)")
}

record( stringin, "$(P):IDN"){
  field( DESC, "SCPI ID String")
  field( DTYP, "stream")
  field( INP,  "@sp3930.proto idn $(L) $(A)")
  field( PINI, "YES")
}

record( stringin, "$(P):ERROR"){
  field( DESC, "Error String")
  field( DTYP, "stream")
  field( SCAN, "5 second")
  field( INP,  "@sp3930.proto getError $(L) $(A)")
  field( PINI, "YES")
}

record(bi,"$(P):BI:LOOP"){
  field( DESC, "Loop Status")
  field( DTYP, "stream")
  field( SCAN, "5 second")
  field( INP,  "@sp3930.proto getLoopStatus $(L) $(A)")
  field( ZNAM, "Off")
  field( ONAM, "On")
  field( PINI, "YES")
}

